{
  "name": "Frank",
  "version": "1.0.0",
  "description": "A minimalist Markdown notes app",
  "main": "main.js",
  "author": "Jonathan James Cosgrove <jjcosgrove.inbox@gmail.com> (http://www.jjcosgrove.com)",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . --icon=app/assets/icons/mac/icon.icns --electron-version=1.6.1 --platform=darwin --arch=x64 --out=builds --overwrite",
    "package-asar": "electron-packager . --icon=app/assets/icons/mac/icon.icns --asar --electron-version=1.6.1 --platform=darwin --arch=x64 --out=builds --overwrite"
  },
  "keywords": [
    "frank",
    "backbone",
    "electron",
    "markdown",
    "nedb",
    "notes",
    "simplemde"
  ],
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone.modelbinder": "^1.1.0",
    "electron-context-menu": "^0.8.0",
    "electron-localshortcut": "^1.1.0",
    "electron-window-state": "^4.0.2",
    "handlebars": "^4.0.6",
    "highlight.js": "^9.10.0",
    "jquery": "^3.1.1",
    "moment": "^2.17.1",
    "nedb": "^1.8.0",
    "semantic-ui-css": "^2.2.9",
    "simplemde": "^1.11.2",
    "split.js": "^1.2.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "standard": "^8.6.0"
  },
  "standard": {
    "globals": [
      "_",
      "$",
      "App",
      "Backbone",
      "CLog",
      "Handlebars",
      "hljs",
      "jQuery",
      "moment",
      "SimpleMDE",
      "Split"
    ]
  }
}
